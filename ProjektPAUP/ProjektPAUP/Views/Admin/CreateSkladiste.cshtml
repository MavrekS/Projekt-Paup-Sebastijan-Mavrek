@model ProjektPAUP.Models.CreateSkladisteModel

<form asp-action="CreateSkladiste" method="post" enctype="multipart/form-data">
      <div asp-validation-summary="ModelOnly" class="text-danger" role="alert"></div>
           <div class="field-wrap">
                <input asp-for="Ime" class="form-control" autocomplete="Ime" aria-required="true" placeholder="Ime" />
                <span asp-validation-for="Ime" class="text-danger"></span>
            </div>
       <button type="submit" >Kreiraj Skladiste</button>

 </form>




@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
